<div class ='container'>

  <div class = 'page-header'>
    <h2>Select a Month</h2>
  </div>

  <div class ='month-index'>
    <% @months.each do |month| %>
        <p class="button">
          <%= link_to(month.name, month_countries_path(month), class: 'btn btn-info btn-lg', role: 'button')%>
        </p>
    <% end %>
  </div>

</div>
