<div class ='container'>

<div class = 'page-header'>
  <h1>Select a Month</h1>
</div>

<div class = 'signInNotDisplay' id = 'signInModalContainer'>
  <%=render partial: '/sessions/signin'%>
</div>

<ul>
<% @months.each do |month| %>
  <p>
    <li><%= link_to(month.name, month_countries_path(month), class: 'btn btn-info btn-lg', role: 'button') %></li>
  </p>
<% end %>
</ul>

</div>
