<div class ='container'>

<div class = 'page-header'>
<h1>Sign in</h1>
</div>

<%= form_for :session, url: session_path do |f| %>
  <p>
  <%= f.label :user_name %><br>
  <%= f.text_field :user_name %><br>
  </p>

  <p>
  <%= f.label :password %><br>
  <%= f.password_field :password%><br>
  </p>

  <p>
  <%= f.submit 'Sign in' %>
  </p>
<% end %>

<%=flash.now[:alert]%>

</div>
